{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:ssr": "tsc && vite build --config ssr.config.ts",
    "build:client": "tsc && vite build --config vite.config.ts",
    "build": "yarn build:ssr && yarn build:client",
    "preview": "vite preview",
    "lint": "npx eslint --config ../../.eslintrc.js --resolve-plugins-relative-to=./ . && yarn stylelint && yarn check-types",
    "stylelint": "stylelint \"**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "check-types": "tsc --project ./tsconfig.json --noEmit",
    "format": "prettier --write .",
    "test": "jest ./"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.1",
    "axios": "1.2.3",
    "dotenv": "16.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.6.0"
  },
  "devDependencies": {
    "@testing-library/react": "13.3.0",
    "@types/jest": "28.1.8",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-test-renderer": "18.0.0",
    "@types/redux-mock-store": "1.0.3",
    "@types/serviceworker": "0.0.61",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "@vitejs/plugin-react": "2.0.1",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^28",
    "jest-canvas-mock": "2.4.0",
    "jest-css-modules-transform": "4.4.2",
    "jest-environment-jsdom": "^28",
    "lefthook": "1.1.1",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "redux-mock-store": "1.5.4",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "1.57.1",
    "stylelint": "14.16.1",
    "stylelint-config-hudochenkov": "8.0.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-order": "5.0.0",
    "ts-jest": "28.0.8",
    "typescript": "4.8.2",
    "vite": "3.0.7"
  },
  "license": "MIT"
}
